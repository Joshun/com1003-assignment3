	public static void processMovement() {
		leftSensorDetect = blackDetected(lineDetect1);
		rightSensorDetect = blackDetected(lineDetect2);

		if( leftSensorDetect && ! rightSensorDetect) {
			goRight();
		}
		if( !leftSensorDetect && rightSensorDetect ) {
			goLeft();
		}
		if( leftSensorDetect && rightSensorDetect ) {
			goForward();
		}
	}